{
  "name": "express",
  "version": "1.0.0",
  "description": "express",
  "main": "server.ts",
  "scripts": {
    "start": "node -r module-alias/register ./dist/server.js",
    "dev": "cp -f .env.development .env && nodemon",
    "build": "npx ts-node ./build.ts"
  },
  "private": "true",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/node": "^20.5.9",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "socket.io": "^4.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.17",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.3",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --files -r tsconfig-paths/register ./src/server.ts | node_modules/.bin/pino-pretty"
  },
  "_moduleAliases": {
    "~": "dist"
  }
}